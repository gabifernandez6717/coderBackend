{{!-- Muestra los productos agregados hasta el momento (al archivo products.json) --}}
<div>
    <h1>
        Productos agregados a products.json:
    </h1>
    {{#each products}}
        <div>
            <h2>
                Title: {{this.title}}
            </h2><br>
            <h2>
                Id: {{this.id}}
            </h2><br>
            <h2>
                description: {{this.description}}
            </h2><br>
            <h2>
                price: {{this.price}}
            </h2><br>
            <h2>
                img: {{this.img}}
            </h2><br>
            <h2>
                code: {{this.code}}
            </h2><br>
            <h2>
                stock: {{this.stock}}
            </h2><br>
            <h2>
                status: {{this.status}}
            </h2><br>
            <h2>
                category: {{this.category}}
            </h2><br>
            <hr>
        </div>
    {{/each}}
</div>